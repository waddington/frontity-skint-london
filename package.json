{
  "name": "skint-london",
  "version": "1.0.0",
  "private": true,
  "description": "Frontity project",
  "keywords": [
    "frontity"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "tailwind": "tailwindcss -i ./packages/skint-2022/src/theme/main.css -o ./packages/skint-2022/src/theme/main-out.css",
    "tailwind:watch": "nodemon --watch ./packages/skint-2022/src --ignore ./packages/skint-2022/src/theme/main-out.css --exec npm run tailwind -e js,mjs,json,yml,css,jsx",
    "dev": "frontity dev --dont-open-browser",
    "build": "frontity build",
    "serve": "frontity serve"
  },
  "prettier": {},
  "dependencies": {
    "@frontity/core": "^1.14.3",
    "@frontity/html2react": "^1.7.0",
    "@frontity/mars-theme": "./packages/mars-theme",
    "@frontity/tiny-router": "^1.4.4",
    "@frontity/wp-source": "^1.11.7",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "frontity": "^1.17.1",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.4",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.1",
    "skint-2022": "file:packages/skint-2022",
    "tailwindcss": "^3.0.0",
    "twin.macro": "^2.8.2"
  },
  "sideEffects": [
    ".css"
  ]
}
